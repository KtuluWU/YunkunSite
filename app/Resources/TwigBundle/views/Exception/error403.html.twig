{# app/Resources/TwigBundle/views/Exception/error403.html.twig #}
{% trans_default_domain 'FOSUserBundle' %}
{% extends 'base_yun.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('css/pages/error_page.css') }}"/>
{% endblock %}

{% set bradcaump_title = 'summury.error.title3'|trans %}
{% set breadcrumb_item = 'summury.error.title4'|trans %}
{% block content %}
    <!-- Start Error PAge -->
