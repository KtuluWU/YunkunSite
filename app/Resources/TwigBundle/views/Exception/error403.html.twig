        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-6 col-md-12 col-sm-12 col-12">
                    <div class="thumb">
                        <img src="{{ asset('images/icons/404.png') }}" alt="yunkun blog">
                    </div>
                </div>
                <div class="col-lg-6 col-md-12 col-sm-12 col-12 sm-mt--30 md-mt--30">
